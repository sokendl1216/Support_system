[run]
source = 
    core
    api
    services
    ai
omit = 
    */tests/*
    */venv/*
    */__pycache__/*
    */migrations/*
    */site-packages/*

[report]
exclude_lines = 
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    raise AssertionError
    if TYPE_CHECKING:

[html]
directory = coverage_html_report
